  "description": "Enhanced Multi-AI Orchestration MCP with OpenRouter and persistent context",
  "private": false,
  "type": "module",
  "main": "./build/index.js",
  "bin": {
    "mcp-multi-ai-pro": "./build/index.js"
  },
  "files": [
